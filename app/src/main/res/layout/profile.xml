<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Clases.logIn">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="83dp"
        android:layout_height="92dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/button_login_selector" />

    <TextView
        android:id="@+id/nameViewP"
        android:layout_width="247dp"
        android:layout_height="18dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="16dp" />

    <TextView
        android:id="@+id/surnameViewP"
        android:layout_width="116dp"
        android:layout_height="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="4dp"
        android:text="Surname"
        app:layout_constraintEnd_toStartOf="@+id/surname2ViewP"
        app:layout_constraintTop_toBottomOf="@+id/nameViewP"
        android:layout_marginRight="4dp" />

    <TextView
        android:id="@+id/surname2ViewP"
        android:layout_width="135dp"
        android:layout_height="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:text="Surname2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameViewP" />

    <TextView
        android:id="@+id/ageView"
        android:layout_width="116dp"
        android:layout_height="21dp"
        android:text="Age"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="90dp" />

    <TextView
        android:id="@+id/genderViewP"
        android:layout_width="131dp"
        android:layout_height="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="Gender"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/surname2ViewP"
        android:layout_marginRight="16dp" />

    <TextView
        android:id="@+id/descView"
        android:layout_width="353dp"
        android:layout_height="103dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="136dp"
        android:layout_marginEnd="16dp"
        android:text="Descripcion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="244dp"
        android:layout_marginEnd="316dp"
        android:text="Address:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/addressView"
        android:layout_width="233dp"
        android:layout_height="19dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="80dp"
        android:text="Adress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/postalView"
        android:layout_width="111dp"
        android:layout_height="16dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="12dp"
        android:text="Postal Code"
        app:layout_constraintEnd_toStartOf="@+id/cityView"
        app:layout_constraintTop_toBottomOf="@+id/addressView" />

    <TextView
        android:id="@+id/cityView"
        android:layout_width="110dp"
        android:layout_height="17dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="80dp"
        android:text="City"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressView" />

    <TextView
        android:id="@+id/lblContact"
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="316dp"
        android:text="Contact:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblAddress" />

    <TextView
        android:id="@+id/phoneView"
        android:layout_width="110dp"
        android:layout_height="17dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="12dp"
        android:text="Phone"
        app:layout_constraintEnd_toStartOf="@+id/typeView"
        app:layout_constraintTop_toBottomOf="@+id/postalView" />

    <TextView
        android:id="@+id/typeView"
        android:layout_width="111dp"
        android:layout_height="17dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="76dp"
        android:text="Phone Type"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cityView" />

    <TextView
        android:id="@+id/emailView"
        android:layout_width="111dp"
        android:layout_height="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="200dp"
        android:text="Email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phoneView" />

    <TextView
        android:id="@+id/lblHoobies"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="320dp"
        android:text="Hobbies:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblContact" />

    <TextView
        android:id="@+id/hobby1View"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="8dp"
        android:text="Hobby1"
        app:layout_constraintEnd_toStartOf="@+id/hobby2View"
        app:layout_constraintHorizontal_bias="0.875"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailView" />

    <TextView
        android:id="@+id/hobby2View"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="143dp"
        android:text="Hobby2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailView" />


</android.support.constraint.ConstraintLayout>